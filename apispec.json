{
  "openapi":"3.0.3",
  "info": {
    "title":"Category RESTful API",
    "description": "Api Spec for Category RESTful API",
    "version":"1.0.0"
  },
  "servers": [
    {
      "url":"https://localhost:3000/api"
    }
  ],
  "paths": {
    "/categories": {
      "get": {
        "description": "List all Categories",
        "responses": {
          "200": {
            "description": "Success get all todolist",
            "content": {
              "aplication/json":{
                "schema":{
                  "type": "object",
                  "properties": {
                    "code":{
                      "type": "number"
                    },
                    "status":{
                      "type": "string"
                    },
                    "data":{
                      "type": "array",
                      "items":{
                        "type": "object",
                        "properties": {
                          "id":{
                            "type": "number"
                          },
                          "name":{
                            "type": "string"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}